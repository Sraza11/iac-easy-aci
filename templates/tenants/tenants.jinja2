
  "{{ keys['name'] }}" = {
{%- for k, v in keys.items()|sort %}
  {%- if k == 'controller_type' and v == 'apic' %}
  {%- elif k == 'controller_type' %}
    {{ k }} = "{{ v }}"
  {%- elif k == 'controller_type' or k == 'class_type' or k == 'name' %}
  {%- elif k == 'policy_type' or k == 'site_group' or k == 'template_type' %}
  {%- elif k == 'tenant' %}
  {%- elif v == None %}
  {%- elif v | int(-1) != -1 %}
    {{ k }} = {{ v }}
  {%- elif v == 'false' or v == 'true' %}
    {{ k }} = {{ v }}
  {%- elif v is iterable and (v is not string and v is not mapping) %}
    {{ k }} = [
    {%- for item in v %}
      {%- if item is mapping %}
      {
        {%- for key, value in item.items() %}
          {%- if key == 'class_type' or key == 'controller_type' or key == 'policy_type' %}
          {%- elif key == 'site_group' or key == 'template_type' or key == 'tenant' %}
          {%- elif value == None %}
          {%- elif value | int(-1) != -1 %}
        {{ key }} = {{ value }}
          {%- elif value == 'false' or value == 'true' %}
        {{ key }} = {{ value }}
          {%- elif value is iterable and (value is not string and value is not mapping) %}
        {{ key }} = [
           {%- for s in value %}
            {%- if not loop.last %}
          "{{ s }}",
            {%- else %}
          "{{ s }}"
            {%- endif %}
           {%- endfor %}
        ]
          {%- elif value is mapping %}
        {{ key }} = [
          {
            {%- for keys, values in value.items() %}
              {%- if values == None %}
              {%- elif values | int(-1) != -1 %}
            {{ keys }} = {{ values }}
              {%- elif values == 'false' or values == 'true' %}
            {{ keys }} = {{ values }}
              {%- else %}
            {{ keys }} = "{{ values }}"
              {%- endif %}
            {%- endfor %}
          }
        ]
          {%- else %}
        {{ key }} = "{{ value }}"
          {%- endif %}
        {%- endfor %}
        {%- if not loop.last %}
      },
        {%- else %}
      }
        {%- endif %}
      {%- elif item | int(-1) != -1 %}
      {{ item }}
      {%- elif item == 'false' or item == 'true' %}
      {{ item }}
      {%- elif not loop.last %}
      "{{ item }}",
      {%- else %}
      "{{ item }}"
      {%- endif %}
    {%- endfor %}
    ]
  {%- else %}
    {{ k }} = "{{ v }}"
  {%- endif %}
{%- endfor %}
  }
