
  "{{ keys['gateway_ip'] }}" = {
{%- for k, v in keys.items() %}
  {%- if k == 'class_type' or k == 'gateway_ip' %}
  {%- elif k == 'policy_type' or k == 'site_group' or k == 'template_type' %}
  {%- elif v == None %}
  {%- elif v | int(-1) != -1 %}
    {{ k }} = {{ v }}
  {%- elif v == 'false' or v == 'true' %}
    {{ k }} = {{ v }}
  {%- elif v is mapping %}
    {{ k }} = [
      {
    {%- for key, value in v.items() %}
      {%- if value == None %}
      {%- elif value | int(-1) != -1 %}
        {{ key }} = {{ value }}
      {%- elif value == 'false' or value == 'true' %}
        {{ key }} = {{ value }}
      {%- elif value is mapping %}
        "{{ key }}" = [
          {
        {%- for keys, values in value.items() %}
          {%- if keys == 'l3out' and values == None %}
            {{ keys }} = ""
          {%- elif values == None %}
          {%- elif values | int(-1) != -1 %}
            {{ keys }} = {{ values }}
          {%- elif values == 'false' or values == 'true' %}
            {{ keys }} = {{ values }}
          {%- else %}
            {{ keys }} = "{{ values }}"
          {%- endif %}
        {%- endfor %}
          }
        ]
      {%- elif value is iterable and (value is not string and value is not mapping) %}
        {{ key }} = [
          {
        {%- for item in value %}
          {%- for keys, values in item.items() %}
            {%- if values == None %}
            {%- elif values | int(-1) != -1 %}
            {{ keys }} = {{ values }}
            {%- elif values == 'false' or values == 'true' %}
            {{ keys }} = {{ values }}
            {%- else %}
            {{ keys }} = "{{ values }}"
            {%- endif %}
          {%- endfor %}
        {%- endfor %}
          }
        ]
      {%- else %}
        {{ key }} = "{{ value }}"
      {%- endif %}
    {%- endfor %}
      }
    ]
  {%- else %}
    {{ k }} = "{{ v }}"
  {%- endif %}
{%- endfor %}
  }
